{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "CommonJS",
		"declaration": true,
		"outDir": "./build",
		"target": "ESNext",
		"allowJs": true,
		"noImplicitAny": true,
		"strict": true,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": ["DOM", "ES5", "ES6", "ES7", "Es2021"],
		"types": ["node", "jest"]
	},
	"include": ["Profiling.ts"],
	"exclude": ["../node_modules", "../tests", "../lib"]
}
